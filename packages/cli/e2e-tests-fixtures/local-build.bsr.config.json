{
  "analyze": {
    "build": {
      "location": "e2e-tests-fixtures/local-build"
    },
    "groups": [
      {
        "key": "html",
        "globs": [
          "index.html"
        ],
        "label": "HTML"
      },
      {
        "key": "js",
        "globs": [
          "@(webpack-runtime|framework|app|commons|711)-*.js",
          "component-*-post-list-*.js"
        ],
        "label": "JS"
      },
      {
        "key": "json",
        "globs": [
          "page-data/*.json"
        ],
        "label": "JSON"
      }
    ],
    "output": "e2e-tests-artifacts/bundle-size-report.json"
  },
  "print": {
    "input": "e2e-tests-artifacts/bundle-size-report.json",
    "output": "e2e-tests-artifacts/bundle-size-report.txt"
  }
}
