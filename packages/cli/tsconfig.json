{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "es2020",
    ],
    "sourceMap": true,
    "allowJs": true,
    "strict": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "importHelpers": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "skipLibCheck": true
  },
}
